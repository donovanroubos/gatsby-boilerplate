{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7
  },
  "env": {
    "node": true,
    "browser": true,
    "jquery": true
  },
  "globals": {},
  "rules": {
    "semi": [2, "never"],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": 2,
    "comma-style": [2, "last"],
    "curly": 2,
    "eqeqeq": 2,
    "func-names": 2,
    "space-before-function-paren": ["error", "never"],
    "guard-for-in": 2,
    "wrap-iife": 2,
    "no-undef": 2,
    "no-use-before-define": [2, "nofunc"],
    "new-cap": 2,
    "quotes": [2, "single"],
    "no-trailing-spaces": 2,
    "max-len": [2, 150],
    "no-underscore-dangle": 0,
    "strict": 0,
    "no-shadow": 0
  }
}
